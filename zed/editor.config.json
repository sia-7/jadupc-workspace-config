{
  "inline_completions": {
    "disabled_globs": [
      "**/.env*",
      "**/*.pem",
      "**/*.key",
      "**/*.cert",
      "**/*.crt",
      "**/secrets.yml"
    ]
  },
  "languages": {
    "JSONC": {
      "show_inline_completions": false
    }
  },
  "assistant": {
    "enabled": false,
    "version": "2"
  },
  "outline_panel": {
    "dock": "right"
  },
  "collaboration_panel": {
    "dock": "right"
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "theme": "Ayu Dark",
  "features": {
    "copilot": false,
    "inline_completion_provider": "supermaven"
  },
  "project_panel": {
    "dock": "right"
  },
  "ui_font_size": 18,
  "ui_font_family": "Comfortaa",
  "buffer_font_size": 22,
  "buffer_line_height": {
    "custom": 1.7
  },
  "autosave": "off",
  "formatter": "language_server",
  "git": {
    "inline_blame": {
      "enabled": true
    },
    "git_gutter": "tracked_files"
  },
  "projects_online_by_default": false,
  "buffer_font_family": "JetBrains Mono",
  "auto_update": false,
  "chat_panel": {
    "button": "never"
  },
  "scrollbar": {
    "show": "always"
  },
  "cursor_blink": false,
  "soft_wrap": "preferred_line_length",
  "toolbar": {
    "selections_menu": false,
    "breadcrumbs": false,
    "quick_actions": false
  },
  "always_treat_brackets_as_autoclosed": true,
  "terminal": {
    "font_family": "JetBrains Mono",
    "font_size": 18,
    "cursor_shape": "bar",
    "shell": "system"
  },
  "base_keymap": "VSCode",
  "gutter": {
    "folds": false
  },
  "file_scan_exclusions": [
    "**/.git",
    "**/node_modules",
    "**/dist",
    "**/.next",
    "**/.svn",
    "**/.hg",
    "**/CVS",
    "**/.DS_Store",
    "**/Thumbs.db",
    "**/.idea",
    "**/.yarn/cache"
  ],
  "show_completion_documentation": false,
  "show_copilot_suggestions": true,
  "show_completions_on_input": true,
  "working_directory": "current_project_directory",
  "show_call_status_icon": false,
  "show_nav_history_buttons": false,
  "node": {
    "path": "/usr/bin/node",
    "npm_path": "/usr/bin/npm",
    "ignore_system_version": false
  },
  "tabs": {
    "file_icons": true,
    "git_status": true
  },
  "use_autoclose": true,
  "cursor_shape": "bar",
  "language_servers": [
    "typescript-language-server",
    "snippet-completion-server",
    "json-language-server",
    "biome"
  ]
}
